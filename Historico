import 'package:flutter/material.dart';
 
void main() {
  runApp(
    MaterialApp(
      title: "Historico",
      debugShowCheckedModeBanner: false,
      home: HistoricoPage(),
    ),
  );
}
 
class HistoricoPage extends StatefulWidget {
  const HistoricoPage({Key key}) : super(key: key);
 
  @override
  _HistoricoPageState createState() => _HistoricoPageState();
}
 
class _HistoricoPageState extends State<HistoricoPageState> {
  @override
  Widget build(BuildContext context) {
    return Scaffold(
      appBar: buildAppBar(),
      body: buildBody(),
 
    );//Scaffold
  }
 
  buildBody(){
    return ListView(
        children: [
          buildListTile(
            urlImage: 'https://www.google.com/url?sa=i&url=https%3A%2F%2Fwww.eonline.com%2Fbr%2Fnews%2F1281098%2Fos-segredos-do-grupo-now-united&psig=AOvVaw3wOHlzJd17oVtBo0G_1Xlo&ust=1629067474919000&source=images&cd=vfe&ved=0CAsQjRxqFwoTCICIorbLsfICFQAAAAAdAAAAABAD'
            nomeContato: 'Diarra',
            msg: 'Senegal',
            data: '01',
          ),
          buildListTile(
            urlImage: 'https://www.google.com/url?sa=i&url=https%3A%2F%2Fwww.eonline.com%2Fbr%2Fnews%2F1281098%2Fos-segredos-do-grupo-now-united&psig=AOvVaw3wOHlzJd17oVtBo0G_1Xlo&ust=1629067474919000&source=images&cd=vfe&ved=0CAsQjRxqFwoTCICIorbLsfICFQAAAAAdAAAAABAD'            
            nomeContato: 'Lamar',
            msg: 'Reino Unido',
            data: '02',
          ),
          buildListTile(
            urlImage: 'https://www.google.com/url?sa=i&url=https%3A%2F%2Fwww.eonline.com%2Fbr%2Fnews%2F1281098%2Fos-segredos-do-grupo-now-united&psig=AOvVaw3wOHlzJd17oVtBo0G_1Xlo&ust=1629067474919000&source=images&cd=vfe&ved=0CAsQjRxqFwoTCICIorbLsfICFQAAAAAdAAAAABAD'
            nomeContato: 'Sabina',
            msg: 'Mexico',
            data: '03',
          ),
          buildListTile(
            urlImage: 'https://www.google.com/url?sa=i&url=https%3A%2F%2Fwww.eonline.com%2Fbr%2Fnews%2F1281098%2Fos-segredos-do-grupo-now-united&psig=AOvVaw3wOHlzJd17oVtBo0G_1Xlo&ust=1629067474919000&source=images&cd=vfe&ved=0CAsQjRxqFwoTCICIorbLsfICFQAAAAAdAAAAABAD'
            nomeContato: 'Any',
            msg: 'Brasil',
            data: '06',
          ),
 
        ],
    ),
  };
 
  buildListTile((String urlImagemString nomeContato, String msg, String data)){//atributos opcionais
      //Text('Linha', style: TextStyle(fontSize: 36)),
          ListTile(
              leading: CircleAvatar(backgroundImage: NetworkImage(urlImagem), 
              radius: 50,
              ),//CircleAvatar
 
              title: Text(
                nomeContato,
                style: TextStyle(fontWeight: FontWeight.bold, fontSize: 18),//negrito
                ),//text
 
              subtitle: Text(msg),
              trailing: Text(
                data,
                style: TextStyle(color: Colors.grey),
                ),
          ),//ListTile
  }
 
  AppBar buildAppBar(){
      return AppBar(
        backgroudColor: Colors.[0x],
        title:  Align(
          alignment: Alignment.centerLeft,
          child: Row(
            children: [
             
Text('Historico), 
        ),//Align
 
      );//AppBar
  }
}
 
